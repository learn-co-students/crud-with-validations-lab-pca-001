<table>
  <thead>
    <tr>
      <th>Song Title</th>
      <th>Artist Name</th>
      <th>Genre</th>
      <th>Release Year</th>
    </tr>
  <thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><p><%= song.title %><br></td>
        <td><%= song.artist_name %><br></td>
        <td><%= song.genre %><br></td>
        <td><%= song.release_year if song.released %></td>
      </tr>
    <% end %>
  </tbody>
</table>